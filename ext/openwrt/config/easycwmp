# easycwmp uci configuration

config local
	option interface eth1.4093
	option port 7547
	option ubus_socket /var/run/ubus.sock
	option date_format %FT%T%z
	option username ''
	option password ''
	option provisioning_code ''
	option logging_level '3'

config acs
	option url http://192.168.4.11:9090/ACS-server/ACS
	option username ''
	option password ''
	option parameter_key ''
	option periodic_enable '1'
	option periodic_interval '100'
	option periodic_time '0001-01-01T00:00:00Z'

config device
	option manufacturer 'EBANG'
	option oui '8CC7D0'
	option product_class 'EB-MIGG'
	option serial_number '17168001234567890'
	option hardware_version 'V0'
	option software_version 'V2.0_201605'
